    {
      "name": "Create Child",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Test Child\",\n  \"dateOfBirth\": \"2019-03-15\",\n  \"age\": 5,\n  \"gender\": \"male\",\n  \"language\": \"en\",\n  \"hospitalId\": \"hospital_a\",\n  \"hospitalName\": \"Hospital A\"\n}"
        },
        "url": {
          "raw": "http://localhost:3000/api/children",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["api", "children"]
