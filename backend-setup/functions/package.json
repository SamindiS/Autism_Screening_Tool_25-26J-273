    "axios": "^1.6.2",
    "pdfkit": "^0.13.0",
    "csv-writer": "^1.6.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1"
